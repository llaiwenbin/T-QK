{
  "scripts": {
    "install-all": "npm-run-all --serial install:*",
    "start": "npm-run-all --parallel start:*",
    "install:master": "cd master && npm i",
    "install:sub1": "cd sub1 && npm i",
    "start:master": "cd master && npm run dev",
    "start:sub1": "cd sub1 && npm run dev"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "__npminstall_done": false
}
